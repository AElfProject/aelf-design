      "import": {
        "types": "./dist/es/index.d.ts",
        "default": "./dist/es/aelf-design.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/aelf-design.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
